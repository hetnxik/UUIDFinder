{
  "manifest_version": 3,
  "name": "UUID Finder",
  "version": "1.0",
  "description": "Find all UUIDs on the current webpage.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["config.json"],
      "matches": ["<all_urls>"]
    }
  ]
}