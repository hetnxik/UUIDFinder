{
  "manifest_version": 3,
  "name": "UUID Finder",
  "version": "1.0",
  "description": "Find all UUIDs on the current webpage.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
